{
  "name": "atlanteans-sale-frontend",
  "version": "1.0.0",
  "private": false,
  "author": "https://gitlab.com/maxschnaider",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "typechain": "rimraf ./src/contracts/types && typechain --target=ethers-v5 **/*/abis/*.json --out-dir=src/contracts/types",
    "postinstall": "yarn typechain",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@rainbow-me/rainbowkit": "^0.11.0",
    "@sentry/nextjs": "^7.39.0",
    "axios": "^1.3.4",
    "ethers": "5.7.1",
    "framer-motion": "^10.0.1",
    "next": "13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wagmi": "^0.11.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@typechain/ethers-v5": "10.1.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "typechain": "^8.1.1",
    "typescript": "4.9.5"
  }
}
